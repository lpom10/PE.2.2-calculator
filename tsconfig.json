{
  // Visit https://aka.ms/tsconfig to read more about this file
  "compilerOptions": {
    
    "module": "commonjs",
    "target": "ES2020",
    "lib": [
      "ES2020"
    ],
    "outDir": "",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
